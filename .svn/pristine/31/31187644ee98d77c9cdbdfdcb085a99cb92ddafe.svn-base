class Estudiante
  # Solo lectura
  attr_reader :nombre
  #attr_reader :nombre, :edad
  # Solo escritura
  #attr_writer :edad
  # Escritura y Lectura
  attr_accessor :edad
  def initialize(nombre, edad)
    @nombre = nombre
    @edad = edad
  end
  def imprimir
    salida = "Nombre: #{@nombre} edad: #{@edad}"
    puts salida
  end
end
